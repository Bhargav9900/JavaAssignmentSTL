package com.sterlite.account;

public class Account {
	
	private int accNo;
	private String ownerName;
	private double balance;
	
	public Account() {
		
		accNo = 1699;
		ownerName = "Bhargav";
		balance = 10000.00;
	}
	
	
	
	

}
