package com.sterlite.calc;

public class Data {

	private int arr[];
	private int total;
	
	

	public Data(int[] arr) {
		//super();
		this.arr = arr;
	}

	public int[] getArr() {
		return arr;
	}
	
	public int getTotal() {
		return total;
	}

	public void setTotal(int total) {
		this.total = total;
	}
	
}
